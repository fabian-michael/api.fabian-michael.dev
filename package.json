{
  "name": "fabian-michael.dev",
  "description": "A blank template to get started with Payload",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "packageManager": "pnpm@8.15.1+sha256.245fe901f8e7fa8782d7f17d32b6a83995e2ae03984cb5b62b8949bfdc27c7b5",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "pnpm copyfiles && pnpm build:payload && pnpm build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload"
  },
  "dependencies": {
    "@lexical/react": "^0.13.1",
    "@lexical/utils": "^0.13.1",
    "@payloadcms/bundler-vite": "^0.1.6",
    "@payloadcms/bundler-webpack": "^1.0.6",
    "@payloadcms/db-mongodb": "^1.4.3",
    "@payloadcms/plugin-cloud": "^3.0.0",
    "@payloadcms/richtext-lexical": "^0.7.0",
    "@payloadcms/richtext-slate": "^1.4.0",
    "clsx": "^2.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "lexical": "^0.13.1",
    "payload": "^2.11.2",
    "react": "^18.2.0",
    "react-select": "^5.8.0",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.4.4",
    "autoprefixer": "^10.4.18",
    "copyfiles": "^2.4.1",
    "daisyui": "^4.7.3",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
